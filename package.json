{
  "name": "opensource.gubanov.eu",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint . --ext .js --ignore-pattern '**/node_modules/**' --ignore-pattern '**/dist/**'",
    "upgrade": "ncu -u && npm i",
    "build": "NODE_ENV=production webpack --mode production",
    "dev": "NODE_ENV=development webpack-dev-server --port 8111 --mode development",
    "deploy": "rimraf dist; deploy-to-git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/opensource.gubanov.eu.git"
  },
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/opensource.gubanov.eu/issues"
  },
  "homepage": "https://github.com/finom/opensource.gubanov.eu#readme",
  "config": {
    "deployToGit": {
      "repository": "https://$GH_TOKEN@github.com/finom/opensource.gubanov.eu.git",
      "branch": "gh-pages",
      "folder": "dist",
      "script": "npm run build",
      "commit": "$npm_package_version",
      "user": {
        "email": "andrey.a.gubanov@gmail.com",
        "name": "Andrey Gubanov (his digital clone)"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-conditional-component": "^0.1.5",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "deploy-to-git": "^0.1.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-check-updates": "^4.0.4",
    "open-browser-webpack-plugin": "0.0.5",
    "optimist": "^0.6.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "skeleton-css": "^2.0.4"
  }
}
