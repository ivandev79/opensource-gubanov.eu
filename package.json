{
  "name": "opensource.gubanov.eu",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint || echo 'Errored'",
    "lint": "eslint . --ext .js --ignore-pattern '**/node_modules/**' --ignore-pattern '**/dist/**'",
    "upgrade": "ncu -a && npm install",
    "build": "rimraf dist; NODE_ENV=production webpack",
    "develop": "NODE_ENV=development webpack-dev-server --port 8111",
    "deploy": "deploy-to-git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/opensource.gubanov.eu.git"
  },
  "author": "Andrey Gubanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/finom/opensource.gubanov.eu/issues"
  },
  "homepage": "https://github.com/finom/opensource.gubanov.eu#readme",
  "config": {
    "deployToGit": {
      "repository": "https://$GH_TOKEN@github.com/finom/opensource.gubanov.eu.git",
      "branch": "gh-pages",
      "folder": "dist",
      "script": "npm run build",
      "commit": "$npm_package_version",
      "user": {
        "email": "andrey.a.gubanov@gmail.com",
        "name": "Andrey Gubanov (his digital clone)"
      }
    }
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "deploy-to-git": "^0.1.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "open-browser-webpack-plugin": "0.0.5",
    "optimist": "^0.6.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "skeleton-css": "^2.0.4"
  }
}
